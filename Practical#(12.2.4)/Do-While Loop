#include<iostream>
using namespace std;
int main(){
	int i,j;
	cout<<"<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<THE DESCRIPTION OF THE CODE>>>>>>>>>>>>>>>>>>>>>>>>>:"<<endl;
	cout<<"When you enter value of i less than that of j "
		  "the code will give output as false, if you do "
		  "opposite, you'll get output as true, infinite times "<<endl;
	cout<<"Enter the value of i"<<endl;
	cin>>i;
	cout<<"Enter the value of j"<<endl;
	cin>>j;
	do{
		cout<<"true"<<endl;
	}
	while(i>j);
	do{
		cout<<"false"<<endl;
	}
	while(i<j);
}
